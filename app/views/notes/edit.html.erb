<h1>Editing Note #<%= @note.id %></h1>

<form action="/notes/<%= @note.id %>/update">
  <div>
    <label for="picture_id">picture_id</label><br />
    <input id="picture_id" name="picture_id" value="<%= @note.picture_id  %>" type="text" />
  </div>
  <div>
    <label for="desc">desc</label><br />
    <input id="desc" name="desc" value="<%= @note.desc  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>